#include "x.hpp"

x::x(void)
{
	std::cout << "Default constructor called." << std::endl;

	return ;
}

x::~x(void)
{
	std::cout << "Destructor called" << std::endl;

	return ;
}

x::x(x const &src)
{
	std::cout << "Copy constructor called" << std::endl;
	*this = src;

	return ;
}

x &x::operator=(x const &rhs)
{
	std::cout << "Copy assignment operator called" << std::endl;

	return (*this);
}
